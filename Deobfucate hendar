https://hendar.infinityfreeapp.com
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local InputBox = Instance.new("TextBox")
local OutputBox = Instance.new("TextBox")
local DecodeButton = Instance.new("TextButton")
local CopyButton = Instance.new("TextButton")
local Title = Instance.new("TextLabel")

ScreenGui.Parent = game.CoreGui

Frame.Parent = ScreenGui
Frame.Size = UDim2.new(0, 600, 0, 450)
Frame.Position = UDim2.new(0.5, -300, 0.5, -225)
Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Frame.Active = true
Frame.Draggable = true

Title.Parent = Frame
Title.Text = "Hendar Deobfuscator GUI"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 22

InputBox.Parent = Frame
InputBox.Size = UDim2.new(1, -20, 0, 160)
InputBox.Position = UDim2.new(0, 10, 0, 50)
InputBox.Text = "Dán script obfuscated ở đây..."
InputBox.ClearTextOnFocus = false
InputBox.MultiLine = true
InputBox.TextWrapped = true
InputBox.TextXAlignment = Enum.TextXAlignment.Left
InputBox.TextYAlignment = Enum.TextYAlignment.Top
InputBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)

OutputBox.Parent = Frame
OutputBox.Size = UDim2.new(1, -20, 0, 160)
OutputBox.Position = UDim2.new(0, 10, 0, 260)
OutputBox.Text = "Code đã decode sẽ hiện ở đây..."
OutputBox.ClearTextOnFocus = false
OutputBox.MultiLine = true
OutputBox.TextWrapped = true
OutputBox.TextXAlignment = Enum.TextXAlignment.Left
OutputBox.TextYAlignment = Enum.TextYAlignment.Top
OutputBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
OutputBox.TextColor3 = Color3.fromRGB(0, 255, 140)

DecodeButton.Parent = Frame
DecodeButton.Size = UDim2.new(0.48, 0, 0, 40)
DecodeButton.Position = UDim2.new(0.02, 0, 1, -45)
DecodeButton.Text = "DECODE"
DecodeButton.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
DecodeButton.TextColor3 = Color3.fromRGB(255, 255, 255)

CopyButton.Parent = Frame
CopyButton.Size = UDim2.new(0.48, 0, 0, 40)
CopyButton.Position = UDim2.new(0.5, 0, 1, -45)
CopyButton.Text = "COPY RESULT"
CopyButton.BackgroundColor3 = Color3.fromRGB(0, 200, 120)
CopyButton.TextColor3 = Color3.fromRGB(255, 255, 255)

DecodeButton.MouseButton1Click:Connect(function()
    local text = InputBox.Text
    local out = {}

    for num in text:gmatch("string%.char%((%d+)%)") do
        table.insert(out, string.char(tonumber(num)))
    end

    if #out == 0 then
        OutputBox.Text = "Không tìm thấy string.char() — script có thể không phải Hendar."
        return
    end

    OutputBox.Text = table.concat(out)
end)

CopyButton.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(OutputBox.Text)
    end
end)
